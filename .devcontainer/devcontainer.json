    "name": "flutter devcontainer",
    "dockerComposeFile": "docker-compose.yml",
    "service": "flutter",
    "workspaceFolder": "/workspace/${workspaceFolderBasename}",
    "customizations": {
        "vscode": {
            "extensions": [
                "Dart-Code.dart-code",
                "Dart-Code.flutter",
                "mhutchie.git-graph"
            ]
        }
    }
}